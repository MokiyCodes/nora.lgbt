{
	"name": "nora.lgbt",
	"description": "a thing uwu",
	"version": "0.0.0",
	"scripts": {
		"build": "preact build",
		"serve": "sirv build --port 8080 --cors --single",
		"dev": "preact watch",
		"lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
		"test": "jest ./tests"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"extends": [
			"preact",
			"plugin:@typescript-eslint/recommended"
		],
		"ignorePatterns": [
			"build/"
		]
	},
	"dependencies": {
		"@nodelib/fs.stat": "2.0.5",
		"preact": "10.5.15",
		"preact-render-to-string": "5.1.19",
		"preact-router": "3.2.1"
	},
	"devDependencies": {
		"@types/enzyme": "3.10.10",
		"@types/jest": "26.0.24",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"css-loader": "6.5.1",
		"css-modules-typescript-loader": "2.0.4",
		"enzyme": "3.11.0",
		"enzyme-adapter-preact-pure": "3.3.0",
		"eslint": "6.8.0",
		"eslint-config-preact": "1.2.0",
		"eslint-plugin-react": "7.27.0",
		"jest": "26.6.3",
		"jest-preset-preact": "4.0.5",
		"preact-cli": "3.3.1",
		"sass": "1.43.4",
		"sass-loader": "10.2.0",
		"sirv-cli": "1.0.14",
		"style-loader": "0.23.1",
		"typescript": "3.9.10",
		"webpack": "5.63.0",
		"webpack-cli": "4.9.1"
	},
	"jest": {
		"preset": "jest-preset-preact",
		"setupFiles": [
			"<rootDir>/tests/__mocks__/browserMocks.ts",
			"<rootDir>/tests/__mocks__/setupTests.ts"
		]
	},
	"private": false,
	"repository": "git@github.com:MokiyCodes/nora.lgbt.git",
	"author": "Mokiy <git@wafflepowered.com>",
	"license": "MIT"
}
